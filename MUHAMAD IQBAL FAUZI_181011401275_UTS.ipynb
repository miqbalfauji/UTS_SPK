#fungsi jumlah_total()
def jumlah_total(daftar_nilai):
    #inisialisasi
    z=0;
    x = daftar_nilai
    jml = len(x)

    #proses iterasi penjumlahan
    #menggunakan fungsi range
    for i in range(jml):
        y = x[i]
        y = int(y)
        z+=y

    #kembalikan hasil
    return z
